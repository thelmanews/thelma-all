{
  "name": "th-google-bar-chart",
  "description": "Multi Bar Chart by Google",
  "category":"chart",
  "version": "0.0.1",
  "thumbnail":"google-bar.jpg", 
  "tags":["chart", "bar"],
  "helpLink":"",
  "width":280,
  "height":200,
  "inputAttr": {
    "gridlines":{"friendly":"Number of Gridlines", "type":"int", "default":"5"},
    "series1Name":{"friendly":"Series 1 Name", "type":"string", "default":"Germany"},
    "series2Name":{"friendly":"Series 2 Name", "type":"string", "default":"France"},
    "series3Name":{"friendly":"Series 3 Name", "type":"string", "default":""},
    "chartData":{"friendly":"Data", "type":"table-repeating","rows":3, 
      "fields":{
        "label":{"friendly":"Label (x)", "type":"string", "default":""},
        "series1":{"friendly":"Series 1 (y)", "type":"int", "default":""},
        "series2":{"friendly":"Series 2 (y)", "type":"int", "default":""},
        "series3":{"friendly":"Series 3 (y)", "type":"int", "default":""}
      },
      "default":[
            {"label": "2012", "series1": 100, "series2": 100 },
            {"label": "2013", "series1": 50, "series2": 150 },
            {"label": "2014", "series1": 300, "series2": 200 },
            {"label": "2015", "series1": 200, "series2": 110 }
        ]
    },
    "backgroundColor":{"friendly":"Background Color", "type":"colorPicker", "default":""},
    "thelma":{"friendly":"Thelma", "type":"hidden", "default": true},
    "legend":{"friendly":"Show Legend?", "type":"boolean", "default": true}
  }
}
